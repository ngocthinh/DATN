<div class="col-md-12">
<div class="col-md-1">
</div>
<div class="col-md-10">
<div class="tab-content">
  <div id="home" class="tab-pane fade in active">
    <h3>All projects</h3>
    <table class="ui selectable inverted table">
    <thead>
      <tr>
        <th>Name</th>
        <th>descriptions</th>
      </tr>
    </thead>
    <tbody>
    <% @projects.each do |project| %>
      <tr>
        <td><%= project.name %></td>
        <td><%= project.description %></td>
        <td><%= image_tag project.images.first.image, class: "img-certificate" %></td>
          <td><%= link_to t(".delete"), admin_project_path(id: project.id), method: :delete, data: { confirm: t("confirm_delete") }, class: "ui inverted red button" %></td>
        	
      </tr>
     <% end %>
    </tbody>
  </table>
  <center><%= paginate @projects, theme: "twitter-bootstrap-3" %></center>
  </div>
</div>
</div>
</div>