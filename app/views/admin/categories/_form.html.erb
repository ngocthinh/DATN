<div class="col-md-12">
  <div class="col-md-3"></div>
  <div class="col-md-6">
    <%= form_for [:admin, category] do |f| %>
      <%= render "shared/error_messages",object: category %>

      <%= f.label :name %>
      <%= f.text_field :name, class: "form-control" %>

      </br>
      <center><%= f.submit "Save", class: "add-account-button" %></center>
    <% end %>
  </div>
  <div class="col-md-3"></div>
</div>
